  <!-- Start Contact section -->
  <section id="mu-contact">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="mu-contact-area">
            <div class="mu-title">
              <!-- <span class="mu-subtitle">Get In Touch</span> -->
              <h2>Liên hệ</h2>
            </div>
            <div class="mu-contact-content">
              <div class="row">
                <div class="col-md-6">
                  <div class="mu-contact-left">
                    <!-- Email message div -->
                    <div id="form-messages"></div>
                    <!-- Start contact form -->
                    <form id="ajax-contact" method="post" action="mailer.php" class="mu-contact-form">
                      <div class="form-group">
                        <label for="name">Họ Và Tên</label>
                        <input type="text" class="form-control" id="name" name="name" placeholder="Họ Và Tên" required>
                      </div>
                      <div class="form-group">
                        <label for="email">Địa Chị Email</label>
                        <input type="email" class="form-control" id="email" name="email" placeholder="Địa Chị Email" required>
                      </div>
                      <div class="form-group">
                        <label for="subject">Tiêu Đề</label>
                        <input type="text" class="form-control" id="subject" name="subject" placeholder="Tiêu Đề" required>
                      </div>
                      <div class="form-group">
                        <label for="message">Nội Dung</label>
                        <textarea class="form-control" id="message" name="message" cols="30" rows="10" placeholder="Nội Dung" required></textarea>
                      </div>
                      <button type="submit" class="mu-send-btn">Gửi</button>
                    </form>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mu-contact-right">
                    <div class="mu-contact-widget">
                      <h3>Địa Chỉ Công Ty</h3>
                      <p>Km29, Đại lộ Thăng Long, Khu công nghệ cao Hòa Lạc, Trường ĐH FPT</p>
                      <address>
                        <p>
                          <i class="fa fa-phone"></i> (+84) 966189407</p>
                        <p>
                          <i class="fa fa-envelope-o"></i>contact@markups.io</p>
                        <p>
                          <i class="fa fa-map-marker"></i>29 Xuân Thủy, Cầu Giấy, Hà Nội</p>
                      </address>
                    </div>
                    <div class="mu-contact-widget">
                      <h3>Khung Giờ Làm Việc</h3>
                      <address>
                        <p>
                          <span>Thứ Hai-Thứ Sáu</span> 8h đến 20h</p>
                        <p>
                          <span>Thứ Bảy</span> 8h đến 20h</p>
                        <p>
                          <span>Chủ Nhật</span> 8h đến 20h</p>
                      </address>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
