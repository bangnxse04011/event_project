  <!-- Start slider  -->
  <section id="mu-slider">
    <div class="mu-slider-area">
      <div class="mu-top-slider">
        <% menu_data.info.forEach(function(info) { %>
          <div class="mu-top-slider-single">
            <img src="<%= info['link'] %>" alt="<%= info['alt'] %>">
            <div class="mu-top-slider-content">
              <span class="mu-slider-small-title">
                <%= info['name'] %>
              </span>
              <br/>
              <br/>
              <br/>
              <p>
                <%= info['details'] %>
              </p>
            </div>
          </div>
          <% }); %>
      </div>
    </div>
  </section>
  <!-- End slider  -->

  <!-- Start About us -->
  <section id="mu-about-us">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="mu-about-us-area">

            <div class="mu-title">
              <!-- <span class="mu-subtitle">Information</span> -->
              <h2 style="color:seagreen">
                <%= menu_data.menu_info %>
              </h2>
            </div>

            <div class="row">
              <div class="col-md-6">
                <div class="mu-about-us-left">
